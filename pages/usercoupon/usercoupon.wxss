/* pages/usercoupon/usercoupon.wxss */
.tabNav{
  z-index: 4; 
  position: fixed; 
  top:0;
  left:0; 
  width:100%; 
  height:70rpx; 
  line-height: 70rpx; 
  background: rgb(253, 253, 253); 
  display: flex; 
  /* padding:0 30rpx;  */
  border-bottom:1px solid #f5f5f5; 
  box-sizing: border-box; 
}
.tabNav> view{
  text-align: center; 
  margin-right:20rpx;
  width: 40%;
}
.tabNav> view:last-child{ 
  margin-right: 0;
}
.tabNav> view text{
  padding:0 15rpx; 
  height:70rpx;
  display:inline-block;
}
.tabNav .cur{ 
  border-bottom:5rpx solid #36ccf9; 
  background: rgb(235, 233, 233); 
  color:rgb(4, 185, 65);
}

/*   优惠券样式  */
.coupon-list{width: 710rpx; margin:  0 auto;}
.coupon-list .item{width: 710rpx; height: 200rpx; margin-bottom: 20rpx;}
.coupon-list .item .float-li{width: 450rpx; height: 100%; border-right: 2rpx dashed rgba(255,255,255,.3)}
.coupon-list .item .float-li-rig{width: 240rpx; padding-right: 20rpx; height:100%; color: #fff;text-align: center;}
.coupon-list .item .float-li-rig .t5 {
  padding-top: 10rpx;
  padding-bottom: 5rpx;
}
.coupon-list .item .float-li-rig .t7 {
  line-height: 30rpx;
  font-size: 30rpx;
  width: 260rpx;
}

.coupon-list .item .float-li-rig .t7-1 {
  line-height: 35rpx;
  font-size: 30rpx;
  width: 260rpx;
  margin: 10rpx 0rpx;
}
 
.coupon-left{position: relative}
.coupon-left .t{position: absolute; color: #fff}
.coupon-left .t1{left: 10rpx; top: 110rpx}
.coupon-left .t2{left: 30rpx; top: 50rpx; font-size: 80rpx; font-weight: bold }
.coupon-left .t3{left: 220rpx; top: 50rpx; width: 200rpx; font-size: 30rpx;}
.coupon-left .t4{left: 10rpx; top:180rpx}
.coupon-left .t5{left: 20rpx; top:20rpx; font-size: 40rpx;}
.coupon-left .t6{margin-left: 20rpx; margin-top: 120rpx; position: absolute; color: white; font-size: 30rpx;}
.coupon-left .t6-1{margin-left: 20rpx; margin-top: 80rpx; position: absolute; color: white; font-size: 30rpx;}
.coupon-left .t7{padding-left: 20rpx; padding-top: 130rpx; position: absolute; color: white; font-size: 25rpx;}
.coupon-left .t8{padding-left: 20rpx; padding-top: 160rpx; position: absolute; color: white; font-size: 25rpx;}

.coupon-rig{
  position: relative;
  padding-top: 20rpx;
}
.coupon-rig .t{display: flex;text-align: center}
.coupon-rig .t1{font-size: 40rpx; padding: 30rpx 0 10rpx 0;}

.coupon-rig .t3 text{width: 100rpx; color: #52f157; border-radius: 7rpx;padding-left: 25rpx;}
.coupon-rig .t4 {text-align: left;}
.coupon-rig .t4 text{padding-left: 50rpx;position: absolute; color: #fff;top:60rpx; font-size: 45rpx;}

.coupon-rig .t8{
  /* z-index: 10; */
  position: absolute;
  /* padding-top: 90rpx; */
  padding-left: 15rpx;
  font-size: 27rpx;
  color: #8c8989;
}

.coupon-rig .t9 {
  color: #5aec65;
  margin-top: 40rpx;
  text-decoration: underline;
}

.coupon-rig .t10 {
  color: #5aec65;
  margin-top: 40rpx;
}


